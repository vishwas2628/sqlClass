<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>User Data</title>
    <style>
        table,
        th,
        td{
            border: 1px solid black;
        }
    </style>
</head>
<body>

    <table>
        <tr>
            <th>Id</th>
            <th>UserName</th>
            <th>Email</th>
        </tr>
      <%   for (user of users){ %>
        <tr>
           <td><%= user.userid %></td> 
           <td><%= user.username %></td>
           <td><%= user.email %></td> 
           <td>
            <form action="/user/<%= user.userid %>/edit" method="get">
                <button>Edit username</button>
            </form>
           </td>
           <td>
            <form action="/user/<%= user.userid %>?_method=DELETE" method="post">
                <button>delete user</button>
            </form>
           </td>

        </tr>
    <% } %>
    </table>
    
</body>
</html>